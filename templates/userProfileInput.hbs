<h2 class="card-title h3">Personal Info:</h2>
<div class="row align-items-center">
    <div class="form-group col-md-4">
        <label for="firstName" class="">First Name</label>
        <input type="text" class="form-control" id="firstName" name="firstName" value="{{firstName}}" aria-describedby="emailHelp">
    </div>
    <div class="form-group col-md-4">
        <label for="exampleInput2" class="">Middle
            Name</label>
        <input type="text" class="form-control" id="exampleInput2" name="middleName" value="{{middleName}}"
            aria-describedby="emailHelp">
    </div>
    <div class="form-group col-md-4">
        <label for="exampleInput3" class="">Last
            Name</label>
        <input type="text" class="form-control" id="exampleInput3" name="lastName" value="lastName" aria-describedby="emailHelp">
    </div>
</div>
<div class="row">
    <div class="form-group col-md-4">
        <label for="primaryEmail" class="">Primary
            Email/Username (can't change)</label>
        <input type="email" class="form-control" id="primaryEmail" name="" value="{{primaryEmail}}" aria-describedby="emailHelp">
    </div>
    <div class="form-group col-md-4">
        <label for="exampleInput5" class="">Secondary
            Email
            (optional)</label>
        <input type="email" class="form-control" id="exampleInput5" name="secondaryEmail" value="{{secondaryEmail}}"
            aria-describedby="emailHelp">
    </div>
    <div class="form-group col-md-4">
        <label for="exampleInput6" class="">Contact
            Number</label>
        <input type="number" class="form-control" id="exampleInput6" name="phone" value="{{phone}}" aria-describedby="emailHelp">
    </div>
</div>
<div class="row">
    <div class="form-group col-12">
        <label for="exampleInput7" class="">Address Line
            1</label>
        <input type="text" class="form-control" id="exampleInput7" name="addressLine1" value="{{addressLine1}}"
            aria-describedby="emailHelp">
    </div>
</div>
<div class="row">
    <div class="form-group col-12">
        <label for="exampleInput8" class="">Address Line
            2</label>
        <input type="text" class="form-control" id="exampleInput8" name="addressLine2" value="{{addressLine2}}"
            aria-describedby="emailHelp">
    </div>
</div>

<div class="row">
    <div class="form-group col-md-4">
        <label for="exampleInput9" class="">City</label>
        <input type="text" class="form-control" id="exampleInput9" name="city" value="{{city}}" aria-describedby="emailHelp">
    </div>



    <div class="form-group  col-md-4">
        <label for="exampleInput10" class="">District</label>
        <input type="text" class="form-control" id="exampleInput10" name="district" value="{{district}}"
            aria-describedby="emailHelp">
    </div>



    <div class="form-group  col-md-4">
        <label for="exampleInput11" class="">State</label>
        <input type="text" class="form-control" id="exampleInput11" name="state" value="{{state}}" aria-describedby="emailHelp">
    </div>
</div>

<div class="row">
    <div class="form-group  col-md-4">
        <label for="exampleInput12" class="">Country</label>
        <input type="text" class="form-control" id="exampleInput12" name="country" value="{{country}}" aria-describedby="emailHelp">
    </div>



    <div class="form-group  col-md-4">
        <label for="exampleInput13" class="">Pin</label>
        <input type="number" class="form-control" id="exampleInput13" name="pin" value="{{pin}}" aria-describedby="emailHelp">
    </div>
    <div class="form-group col-md-4">
        <label for="exampleInput14" class="">Date of birth</label>
        <input type="date" class="form-control" id="exampleInput14" name="dateOfBirth" value="{{dateOfBirth}}"
            aria-describedby="emailHelp">
    </div>
</div>
<div class="row px-3">
    <div class="col-12">
        <div class="row">
            <p class="text-muted">Gender</p>
        </div>
        <div class="row justify-content-around">
            <div class="form-check form-check-radio">
                <label class="form-check-label">
                    <input class="form-check-input" type="radio" name="isMale" id="exampleRadios2" value="true"
                        aria-describedby="emailHelp" {{maleChecked}}>
                    Male
                    <span class="circle">
                        <span class="check"></span>
                    </span>
                </label>
            </div>
            <div class="form-check form-check-radio ">
                <label class="form-check-label">
                    <input class="form-check-input" type="radio" name="isMale" id="exampleRadios1" value="false"
                        aria-describedby="emailHelp" {{femaleChecked}}>
                    Female
                    <span class="circle">
                        <span class="check"></span>
                    </span>
                </label>
            </div>
            <div class="form-check form-check-radio ">
                <label class="form-check-label">
                    <input class="form-check-input" type="radio" name="isMale" id="exampleRadios0" value="other"
                        aria-describedby="emailHelp" {{otherChecked}}>
                    Other
                    <span class="circle">
                        <span class="check"></span>
                    </span>
                </label>
            </div>
        </div>
    </div>

</div>
<div class="row px-3">
    <div class="col-12">

        <div class="row">
            <p class="text-muted">Role</p>
        </div>

        <div class="row justify-content-around">

            <div class="form-check form-check-radio">
                <label class="form-check-label">
                    <input class="form-check-input" type="radio" name="primaryRole" id="exampleRadios3" value="student"
                        aria-describedby="emailHelp" {{studentChecked}}>
                    Student
                    <span class="circle">
                        <span class="check"></span>
                    </span>
                </label>
            </div>

            <div class="form-check form-check-radio">
                <label class="form-check-label">
                    <input class="form-check-input" type="radio" name="primaryRole" id="exampleRadios4" value="parent"
                        aria-describedby="emailHelp" {{parentChecked}}>
                    Parent
                    <span class="circle">
                        <span class="check"></span>
                    </span>
                </label>
            </div>

            <div class="form-check form-check-radio">
                <label class="form-check-label">
                    <input class="form-check-input" type="radio" name="primaryRole" id="exampleRadios5" value="enterprise"
                        {{instituteChecked}}>
                    Institute
                    <span class="circle">
                        <span class="check"></span>
                    </span>
                </label>
            </div>

        </div>

    </div>
</div>