<div class="tab-pane" id="tab{{tabNumber}}">
    <div class="card bg-info mt-2">
        <div class="card-body px-3">
            <h3 class="card-title text-dark" data-id="{{_id}}" data-tabNumber="{{tabNumber}}">
                {{name}}
            </h3>
            <h5 class="card-title">
                Study Monitor
            </h5>
        </div>
    </div>
    <ul class="nav nav-pills nav-pills-rose">
        <li class="nav-item"><a class="nav-link active" href="#pill1{{tabNumber}}" data-toggle="tab">Configure</a></li>
        <li class="nav-item"><a class="nav-link" href="#pill2{{tabNumber}}" data-toggle="tab">Promoter</a></li>
        <li class="nav-item"><a class="nav-link" href="#pill3{{tabNumber}}" data-toggle="tab">Communicator</a></li>
        <li class="nav-item"><a class="nav-link" href="#pill4{{tabNumber}}" data-toggle="tab">Schedule</a></li>
        <li class="nav-item"><a class="nav-link" href="#pill5{{tabNumber}}" data-toggle="tab">Attendance</a></li>
    </ul>
    <hr>
    <div class="tab-content tab-space">
        <div class="tab-pane active" id="pill1{{tabNumber}}">
            <div class="row m-0">
                <div class="col-md-2" style="border-right: 1px solid rgba(0, 0, 0, 0.1)">
                    <ul class="nav nav-pills nav-pills-rose flex-column">
                        <li class="nav-item"><a class="nav-link active" href="#innertab1{{tabNumber}}" data-toggle="tab">Courses</a></li>
                        <li class="nav-item"><a class="nav-link" href="#innertab3{{tabNumber}}" data-toggle="tab">Batches</a></li>
                        <li class="nav-item"><a class="nav-link" href="#innertab2{{tabNumber}}" data-toggle="tab">Students</a></li>
                        <li class="nav-item"><a class="nav-link" href="#innertab4{{tabNumber}}" data-toggle="tab">Active
                                Batches</a></li>
                    </ul>
                </div>
                <div class="col-md-10">
                    <div class="tab-content">
                        <div class="tab-pane active" id="innertab1{{tabNumber}}">
                            <div class="row m-0">
                                <div class="card-title col-12 h3">
                                    Active Courses
                                </div>
                                <div class="col-12">
                                    <div class="row">
                                        {{#each courses as |course|}}
                                        <div class="col-md-4" id="{{course._id}}">
                                            <div class="card">
                                                <div class="card-body ">
                                                    <h4 class="card-title" data-toggle="modal" data-target="#editCourseModal{{course._id}}">
                                                        <a href="#pablo">{{course.code}}</a>
                                                    </h4>
                                                </div>
                                                <div class="card-footer ">
                                                    <div class="author">
                                                        <h6 class="card-category text-danger">
                                                            {{!-- <i class="material-icons">trending_up</i> --}}
                                                            Price {{course.fees}}/-
                                                        </h6>
                                                    </div>
                                                    <div class="stats ml-auto">
                                                        <i class="material-icons delete-course-btn" style="top: 0%; cursor: pointer;"
                                                            data-id="{{course._id}}" data-parent="{{course.parentId}}">delete</i>
                                                        <i class="material-icons" style="top: 0%; cursor: pointer;">edit</i>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Modal -->
                                        <form class="edit_course_form" data-tabNumber="{{tabNumber}}" data-tuition="{{course.parentId}}"
                                            data-course="{{course._id}}" data-modal="editCourseModal{{course._id}}"
                                            enctype="multipart/form-data">
                                            <div class="modal fade bd-example-modal-lg" id="editCourseModal{{course._id}}"
                                                tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
                                                aria-hidden="true">
                                                <div class="modal-dialog modal-lg" role="document">
                                                    <div class="modal-content">
                                                        <div class="modal-body p-0">
                                                            <div class="card my-0 bg-info">
                                                                <div class="card-body pt-0 pb-0">
                                                                    <h3 class="card-title my-2">
                                                                        <a href="#pablo" class="text-dark">{{course.code}}</a>
                                                                    </h3>
                                                                </div>
                                                            </div>
                                                            <div class="card my-0">
                                                                <div class="card-body ">
                                                                    <div class="row align-items-center justify-content-around">
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0 border border-white">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                        COURSE CODE
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    <input name="code" type="text"
                                                                                        class="form-control bg-white"
                                                                                        placeholder="COURSE CODE" value="{{course.code}}">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                        DESCRIPTION
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    <input name="description" type="text"
                                                                                        class="form-control bg-white"
                                                                                        placeholder="DESCRIPTION" value="{{course.description}}">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0 border border-white">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                        COURSE FEE
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    <input name="fees" type="text"
                                                                                        class="form-control bg-white fee-inp"
                                                                                        placeholder="COURSE FEE" value="{{course.fees}}"
                                                                                        data-tabNumber="{{course._id}}">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0 border border-white">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    <div class="row">
                                                                                        <div class="col-md-6">
                                                                                            <div class="form-check">
                                                                                                <label class="form-check-label">
                                                                                                    <input class="form-check-input gst-checkbox"
                                                                                                        type="checkbox"
                                                                                                        data-tabNumber="{{course._id}}">
                                                                                                    INCLUSIVE OF GST
                                                                                                    <span class="form-check-sign">
                                                                                                        <span class="check"></span>
                                                                                                    </span>
                                                                                                </label>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="col-md-6">
                                                                                            <div class="row">
                                                                                                <div class="col-md-3 pl-0 pr-0">
                                                                                                    <input name="gst"
                                                                                                        type="text"
                                                                                                        class="form-control bg-white border rounded  mt-3 gst-inp"
                                                                                                        style="height:24px;"
                                                                                                        value="{{course.gst}}"
                                                                                                        data-tabNumber="{{course._id}}">
                                                                                                </div>
                                                                                                <div class="col-md-9 pl-0 pr-0 mt-3">
                                                                                                    <p>GST PERCENTAGE</p>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="row align-items-center justify-content-around">
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0 border border-white">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                        TOTAL FEE
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    <input type="text" class="form-control bg-white total-fee"
                                                                                        placeholder="TOTAL FEE"
                                                                                        data-tabNumber="{{course._id}}">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0 border border-white">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                        DEFINED BATCHES
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    {{#each batches as |batch|}}
                                                                                    <span class="badge badge-info">{{batch.code}}</span>
                                                                                    {{/each}}
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <br>
                                                                    <div class="col-sm-12 col-xs-12 mt-3 ml-3 text-right">
                                                                        <button type="button" class="btn btn-danger"
                                                                            disabled>CHANGE
                                                                            STATUS</button>
                                                                        <button type="submit" class="btn btn-primary">Save
                                                                            changes</button>
                                                                        <button type="button" class="btn btn-secondary"
                                                                            data-dismiss="modal">Close</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                        {{/each}}
                                    </div>
                                    <div id="active_course_container{{tabNumber}}" class="row">
                                    </div>
                                </div>
                                {{!-- <div class="card-title col-12 h3">
                                    Inactive Courses
                                </div> --}}
                                <div class="inactive-course-container"></div>
                                <div class="card m-0 mb-4">
                                    <form class="new_course_form" data-tabNumber="{{tabNumber}}" data-id="{{_id}}">
                                        <div class="row card-body">
                                            <div class="card-title h3 col-12">
                                                Add New Course/Product
                                            </div>
                                            <div class="col-md-6">
                                                {{!-- <div class="form-group label-floating is-empty has-rose bmd-form-group">
                                                    <input type="text" name="code" class="form-control" placeholder="Course Name"
                                                        required>
                                                    <span class="material-input"></span>
                                                </div> --}}
                                                <div class="form-group label-floating is-empty has-rose bmd-form-group">
                                                    <input id="course-code" name="code" class="form-control" type="text"
                                                        placeholder="Course Code (unique)" required>
                                                    <span class="material-input"></span>
                                                </div>
                                                <div class="form-group label-floating has-rose is-empty bmd-form-group">
                                                    <textarea name="description" class="form-control" rows="4"
                                                        placeholder="Description (optional)"></textarea>
                                                    <span class="material-input"></span>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group label-floating is-empty has-rose bmd-form-group">
                                                    <input name="fees" type="number" class="form-control fee-inp"
                                                        placeholder="Course Fee" required data-tabNumber="{{tabNumber}}">
                                                    <span class="material-input"></span>
                                                </div>
                                                <div class="form-check">
                                                    <label class="form-check-label">
                                                        <input class="form-check-input gst-checkbox" type="checkbox"
                                                            value="true" data-tabNumber="{{tabNumber}}">
                                                        insclusive of taxes
                                                        <span class="form-check-sign">
                                                            <span class="check"></span>
                                                        </span>
                                                    </label>
                                                </div>
                                                <div class="form-group label-floating has-rose is-empty bmd-form-group">
                                                    <input name="gst" type="number" class="form-control gst-inp"
                                                        placeholder="GST (%)" data-tabNumber="{{tabNumber}}">
                                                    <span class="material-input"></span>
                                                </div>
                                                <div class="form-group label-floating has-rose is-empty bmd-form-group">
                                                    <label>Total Fee</label>
                                                    <input type="number" class="form-control total-fee" placeholder=""
                                                        data-tabNumber="{{tabNumber}}">
                                                    <span class="material-input"></span>
                                                </div>
                                                {{!-- <p class="font-weight-bold">Total Fee = <span class="total-fee"
                                                        data-tabNumber="{{tabNumber}}"></span></p> --}}
                                            </div>
                                            <div class="col-md-6">
                                            </div>
                                            <div class="col-md-6">
                                                <button type="submit" class="btn btn-round btn-rose pull-right">
                                                    ADD Course
                                                    <div class="ripple-container"></div>
                                                </button>
                                            </div>
                                        </div>
                                        {{!-- <input type="hidden" name="listingId" value="{{_id}}">
                                        <input type="hidden" name="listingCategory" value="{{category}}"> --}}
                                    </form>
                                </div>
                            </div>
                            <br><br>
                        </div>
                        <div class="tab-pane" id="innertab2{{tabNumber}}">
                            <div class=" card-title col-12 h3 mt-4">
                                Active Students
                            </div>
                            <div class="container">
                                <div class="section section-comments p-1">
                                    <div class="row">
                                        {{#each students}}
                                        <div class="col-md-4" id="{{_id}}">
                                            <div class="media-area main-raised m-3">
                                                <div class="media">
                                                    <a class="float-left" href="#pablo">
                                                        <div class="avatar">
                                                            <img class="media-object" src="../assets/img/logo.png" alt="...">
                                                        </div>
                                                    </a>
                                                    <div class="media-body" style="overflow: hidden">
                                                        <h5 class="media-heading mb-0 pb-0">{{name}}
                                                        </h5>
                                                        <small class="text-danger">{{rollNumber}}</small>
                                                        <div class="stats ml-auto text-secondary">
                                                            <i class="material-icons delete-student-btn" style="top: 0%; cursor: pointer;"
                                                                data-id="{{_id}}" data-tuition="{{tuitionId}}">delete</i>
                                                            <i class="material-icons" style="top: 0%; cursor: pointer;">edit</i>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        {{/each}}
                                    </div>
                                    <div class="row mt-2" id="active_students_container{{tabNumber}}"></div>
                                </div>
                            </div>
                            <div class="card card-profile card-plaina main-raised m-0">
                                <div class="row card-body">
                                    <div class="col-md-6">
                                        <h4 class="card-title text-left">Bulk Upload</h4>
                                        <p class="description text-left">use the excel
                                            template
                                            to bulk
                                            Upload your student data please do not edit
                                            template</p>
                                    </div>
                                    <div class="col-md-6">
                                        <button class="btn btn-round btn-rose float-right">Download
                                            Template</button>
                                        <button class="btn btn-round btn-rose float-right">Upload
                                            Template</button>
                                    </div>
                                </div>
                            </div>
                            <br>

                            <div class="card card-profile main-raised m-0">
                                <form class="new_student_form" data-tabNumber="{{tabNumber}}" data-id="{{_id}}">
                                    <div class="card-body text-left has-rose">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <h4 class="card-title">Auto Fields</h4>
                                                <input name="eduatlasId" type="text" placeholder="EA Membership number"
                                                    class="form-control" disabled>
                                                <h4 class="card-title">Compulsory Fields</h4>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <input name="rollNumber" type="text" placeholder="Student Roll No."
                                                            class="form-control" required>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input name="name" type="text" placeholder="Student Name" class="form-control"
                                                            required>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input name="email" type="text" placeholder="Student E-mail"
                                                            class="form-control" required>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <h4 class="card-title">Additional Fields</h4>
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <input name="address" type="text" placeholder="Address" class="form-control" />
                                                    </div>
                                                    <div class="col-md-6">
                                                        <input name="contactNumber" type="text" placeholder="Contact No."
                                                            class="form-control" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <h4 class="card-title">Payment Details</h4>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="-Product Name" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="Products Fees" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="Discount Amount Given" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="Discount Reason" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="Net Fee" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="Fee Collected" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="Mode Of Payment" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="Balance Pending" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="next installment details" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="print receipt" class="form-control" />
                                                    </div>
                                                    <div class="col-md-4">
                                                        <input type="text" placeholder="add to batch code" class="form-control" />
                                                    </div>
                                                    <div class="col-md-12 mt-3">
                                                        <button class="btn btn-round btn-rose float-right">ADD
                                                            STUDENTS</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                        <div class="tab-pane" id="innertab3{{tabNumber}}">
                            <div class="row m-0">
                                <div class="card-title col-12 h3">
                                    Active Batches
                                </div>
                                <div class="col-12">
                                    <div class="row">
                                        {{#each batches}}
                                        <div class="col-md-4" id="{{_id}}">
                                            <div class="card">
                                                <div class="card-body ">
                                                    <h4 class="card-title" data-toggle="modal" data-target="#editBatchModal{{_id}}">
                                                        <a href="#pablo">{{code}}</a>
                                                    </h4>
                                                    <p class="card-text">
                                                        {{description}}
                                                    </p>
                                                    <p class="card-text">
                                                        <small class="text-secondary">Course:</small>
                                                        <small class="text-danger">{{parentCourse}}</small>
                                                    </p>
                                                </div>
                                                <div class="card-footer ">
                                                    <div class="stats ml-auto">
                                                        <i class="material-icons delete-batch-btn" style="top: 0%; cursor: pointer;"
                                                            data-id="{{_id}}" data-tuition="{{tuitionId}}" data-course="{{courseId}}">delete</i>
                                                        <i class="material-icons" style="top: 0%; cursor: pointer;">edit</i>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <form class="edit_batch_form" data-tabNumber="{{tabNumber}}" data-tuition="{{tuitionId}}"
                                            data-course="{{courseId}}" data-batch="{{_id}}" data-modal="editBatchModal{{_id}}"
                                            enctype="multipart/form-data">
                                            <div class="modal fade bd-example-modal-lg" id="editBatchModal{{_id}}"
                                                tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
                                                aria-hidden="true">
                                                <div class="modal-dialog modal-lg" role="document">
                                                    <div class="modal-content">
                                                        <div class="modal-body p-0">
                                                            <div class="card my-0 bg-info">
                                                                <div class="card-body pt-0 pb-0">
                                                                    <h3 class="card-title my-2">
                                                                        <a href="#pablo" class="text-dark">{{code}}</a>
                                                                    </h3>
                                                                </div>
                                                            </div>
                                                            <div class="card my-0">
                                                                <div class="card-body ">
                                                                    <div class="row align-items-center justify-content-around">
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0 border border-white">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                        BATCH CODE
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    <input name="code" type="text"
                                                                                        class="form-control bg-white"
                                                                                        placeholder="BATCH CODE" value="{{code}}">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                        DESCRIPTION
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    <input name="description" type="text"
                                                                                        class="form-control bg-white"
                                                                                        placeholder="DESCRIPTION" value="{{description}}">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                        PARENT COURSE
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    <p>{{parentCourse}}</p>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="row align-items-center justify-content-around">
                                                                        <div class="col-sm-12 col-xs-12 mt-1">
                                                                            <div class="row">
                                                                                <div class="col-md-4 pt-0 pb-0 border border-white">
                                                                                    <span class="input-group-addon"
                                                                                        style="font-size: 12px;">
                                                                                        STUDENTS
                                                                                    </span>
                                                                                </div>
                                                                                <div class="col-md-8 p-0">
                                                                                    <div class="px-2" style="height:299px;background-color:rgb(184, 184, 184);overflow-y: auto;max-height:100px;">
                                                                                        <table class="table">
                                                                                            <thead>
                                                                                                <tr>
                                                                                                    <th class="text-center">#</th>
                                                                                                    <th>Name</th>
                                                                                                    <th>Roll Number</th>
                                                                                                    <th>Email</th>
                                                                                                    <th>Actions</th>
                                                                                                </tr>
                                                                                            </thead>
                                                                                            <tbody>
