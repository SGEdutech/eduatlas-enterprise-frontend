{{#each students as |student|}}
<tr>
    <td class="text-center p-0">{{inc @index}}</td>
    <td class=" p-0">{{name}}</td>
    <td class=" p-0">{{rollNumber}}</td>
    <td class=" p-0">{{email}}</td>
    <td class=" p-0">
        <input name="students" type="checkbox" value="{{email}}" data-student-id="{{_id}}" data-tuition-id="{{tuitionId}}">
    </td>
</tr>
{{/each}}