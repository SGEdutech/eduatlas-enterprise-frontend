{{#each students as |student|}}
<tr>
    <td class="text-center p-0">{{inc @index}}</td>
    <td class="p-0 text-capitalize">{{name}}</td>
    <td class="p-0 text-capitalize">{{rollNumber}}</td>
    <td class="p-0">{{email}}</td>
    <td class="p-0">
        <div class="form-check m-0">
            <label class="form-check-label">
                <input class="form-check-input" name="students" type="checkbox" value="{{email}}" data-student-id="{{_id}}"
                    data-tuition-id="{{tuitionId}}">
                <span class="form-check-sign">
                    <span class="check"></span>
                </span>
            </label>
        </div>
    </td>
</tr>
{{/each}}